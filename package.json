{
  "name": "@markvachi/bash-ai",
  "version": "1.0.16",
  "main": "bin/bash-ai",
  "author": "markvahi",
  "bin": {
    "??": "bin/bash-ai"
  },
  "license": "MIT",
  "scripts": {
    "start": "ts-node src/app.ts",
    "dev": "ts-node-dev --respawn src/app.ts",
    "clean": "tsc --build --clean",
    "build": "tsc",
    "postinstall": "npm run clean && npm run build",
    "prepublishOnly": "npm run clean && npm run build",
    "preversion": "npm run clean && npm run build",
    "version": "npm run clean && npm run build",
    "postversion": "git push && git push --tags"
  },
  "exports": {
    ".": {
      "import": "./dist/app.js",
      "require": "./dist/app.js"
    },
    "./package.json": "./package.json"
  },
  "dependencies": {
    "commander": "^10.0.1",
    "openai": "^3.2.1"
  },
  "devDependencies": {
    "@types/node": "^18.15.11",
    "ts-node-dev": "^2.0.0"
  },
  "description": "Bash AI is a command-line tool that uses the power of OpenAI's GPT-3 to provide intelligent suggestions and automation for your bash commands.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/M4R14/bash-ai.git"
  },
  "keywords": [
    "gpt-3",
    "ai",
    "open-ai"
  ],
  "bugs": {
    "url": "https://github.com/M4R14/bash-ai/issues"
  },
  "homepage": "https://github.com/M4R14/bash-ai#readme",
  "release": {
    "branches": [
      "main"
    ]
  },
  "publishConfig": {
    "access": "public"
  }
}